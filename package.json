{
  "name": "ts-json-properties",
  "version": "2.0.1",
  "description": "Use Typescript decorators to retrieve a property from properties.json and load it on class attribute.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf 'test/**/*.{js,js.map}' 'src/**/*.{js,js.map}'",
    "tslint": "tslint 'src/**/*.ts?(x)'",
    "test": "npm run clean && npm run tsc && npm run tslint && NODE_ENV=test nyc --reporter=html --reporter=text _mocha --recursive",
    "coveralls:travis": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:compile": "rimraf ./lib && tsc --project tsconfig.compile.json",
    "doc:serve": "concurrently \"tsc --project tsconfig.compile.json -w\" \"docsify serve ./docs\" \"cd docs/_build && nodemon --delay 2.5 -e ts --watch ../../lib build-api.js\"",
    "doc:build": "exit 0",
    "prepublish": "npm run tsc:compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romakita/json-properties.git"
  },
  "keywords": [
    "ts",
    "typescript",
    "properties",
    "immutable",
    "decorator",
    "json"
  ],
  "author": "Romakita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romakita/json-properties/issues"
  },
  "homepage": "https://github.com/romakita/json-properties",
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.54",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.4.0",
    "semantic-release": "^7.0.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.0"
  },
  "release": {
    "debug": false,
    "branch": "production"
  },
  "gflow": {
    "master": "master",
    "production": "production"
  }
}